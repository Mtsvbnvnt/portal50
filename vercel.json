{
  "buildCommand": "cd Frontend_Portal50 && npm install && npm run build",
  "outputDirectory": "Frontend_Portal50/dist",
  "cleanUrls": true,
  "trailingSlash": false,
  "env": {
    "VITE_API_URL": "@portal50_api_url"
  },
  "build": {
    "env": {
      "VITE_API_URL": "@portal50_api_url"
    }
  },
  "functions": {
    "Backend_Portal50/src/index.ts": {
      "runtime": "@vercel/node",
      "maxDuration": 30
    },
    "api/**/*.ts": {
      "runtime": "@vercel/node",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/Backend_Portal50/src/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}